
@{
    ViewBag.Title = "ManageStatusParkingPlace";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Tình trạng xe trong bãi đỗ</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                        <li class="breadcrumb-item active">Tình trạng bãi đỗ xe</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">

                            <div class="dropdown">
                                <strong style="font-size:x-large">Bãi đỗ     </strong>
                                <button class="btn btn-primary dropdown-toggle" style="margin-top:-10px" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    FPT Place
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">FPT Place</a>
                                    <a class="dropdown-item" href="#">Fsoft-1</a>
                                    <a class="dropdown-item" href="#">Fsoft-2</a>
                                </div>
                            </div><br />
                            <div class="row invoice-info">
                                <div class="col-sm-2">
                                    <strong>Đã gửi: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Xe máy: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Ô tô: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-2">
                                    123
                                </div>
                            </div><hr class="solid" style="width:60%;margin-right:40%" />
                            <div class="row invoice-info">
                                <div class="col-sm-2">
                                    <strong>Chỗ trống: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Xe máy: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Ô tô: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-2">
                                    123
                                </div>
                            </div><hr class="solid" style="width:60%;margin-right:40%" />
                            <div class="row invoice-info">
                                <div class="col-sm-2">
                                    <strong>Số vé tháng: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Xe máy: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Ô tô: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-2">
                                    123
                                </div>
                            </div><hr class="solid" style="width:60%;margin-right:40%" />
                            <div class="row invoice-info">
                                <div class="col-sm-2">
                                    <strong>Biểu phí bãi đỗ: </strong>
                                    <br />(Theo giờ/block)
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Xe máy: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-1">
                                    123
                                </div>
                                <div class="col-sm-1">
                                    <strong>Ô tô: </strong>
                                    <br />
                                </div>
                                <div class="col-sm-2">
                                    123
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-9">
                                    <form autocomplete="off">
                                        <div class="flex-row d-flex justify-content-center">
                                            <a>From: </a><input style="margin-left:10px;margin-top:-5px" type="time" value="From" />
                                            <a style="margin-left:20px">To: </a><input style="margin-left:10px;margin-top:-5px" type="time" value="To" />
                                        </div>
                                    </form>
                                </div>
                                <div class="col-md-3">
                                    <form class="form-inline active-purple-3 active-purple-4">
                                        <i class="fas fa-search" aria-hidden="true"></i>
                                        <input class="form-control form-control-sm ml-3 w-75" type="text" placeholder="Search" aria-label="Search">
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <table id="example2" class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>Biển số</th>
                                        <th>Thời gian vào</th>
                                        <th>Thời gian ra</th>
                                        <th>Hình thức vé</th>
                                        <th>Số thẻ</th>
                                        <th>Loại xe</th>
                                        <th>Bãi đỗ</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in ViewBag.transac)

                                    {
                                        string TypeOfTicket = string.Empty;
                                        switch (item.TypeOfTicket)
                                        {
                                            case 0:
                                                TypeOfTicket = "Vé Tháng";
                                                break;
                                            case 1:
                                                TypeOfTicket = "Vé Lượt";
                                                break;

                                        }
                                        <tr>

                                            <td>@item.LicensePlates</td>
                                            <td>@item.TimeIn</td>
                                            <td>@item.TimeOutv</td>
                                            <td>@TypeOfTicket</td>
                                            <td>@item.Card.CardNumber</td>
                                            <td>@item.LicensePlates</td>
                                            <td>@item.ParkingPlace.NameOfParking</td>
                                        </tr>
                                    }
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>Biển số</th>
                                        <th>Thời gian vào</th>
                                        <th>Thời gian ra</th>
                                        <th>Hình thức vé</th>
                                        <th>Số thẻ</th>
                                        <th>Loại xe</th>
                                        <th>Bãi đỗ</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                        <!-- /.card-body -->
                    </div>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->