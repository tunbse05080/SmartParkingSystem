                                <table class="table table-avatar">
                                    <thead>
                                        <tr class="bg-green">
                                            <th></th>
                                            <th>Đã gửi</th>
                                            <th>Chỗ trống</th>
                                            <th>Số vé tháng</th>
                                            <th>Số vé ngày</th>
